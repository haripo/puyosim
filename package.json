{
  "name": "puyosim",
  "version": "0.0.1",
  "scripts": {
    "android:start": "concurrently \"node node_modules/react-native/local-cli/cli.js start --reset-cache\" \"react-native run-android --no-packager --appIdSuffix debug\"",
    "android:build": "cd android && ./gradlew app:assembleRelease && ./gradlew installRelease",
    "android:emulator": "cd $ANDROID_HOME/emulator && ./emulator -avd \"Nexus_5X_API_27_x86\"",
    "ios:start": "concurrently \"node node_modules/react-native/local-cli/cli.js start --reset-cache\" \"react-native run-ios --no-packager\"",
    "native:typecheck": "tsc",
    "web:start": "NODE_ENV=development webpack-dev-server --mode development --config web/webpack.config.js --watch",
    "web:build": "NODE_ENV=production webpack --config web/webpack.config.js --mode production",
    "web:deploy": "npm run web:build && firebase deploy --only hosting:production",
    "web:stg-deploy": "npm run web:build && firebase deploy --only hosting:staging",
    "functions:serve": "cd functions && npm run dev-build && cd .. && firebase serve",
    "functions:deploy": "cd functions && npm run build && cd .. && firebase deploy --only functions:production",
    "functions:stg-deploy": "cd functions && npm run build && cd .. && firebase deploy --only functions:staging",
    "test": "jest"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@react-native-community/async-storage": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "@typescript-eslint/typescript-estree": "^1.7.0",
    "eslint": "^5.16.0",
    "firebase-functions": "^3.1.0",
    "i18n-js": "^3.2.1",
    "immer": "^3.1.1",
    "lodash": "^4.17.19",
    "native-base": "^2.13.0",
    "query-string": "^6.4.2",
    "raven-for-redux": "^1.4.0",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-hotkeys": "^1.1.4",
    "react-mixin": "^5.0.0",
    "react-modal": "^3.8.1",
    "react-native": "0.59.5",
    "react-native-dotenv": "^0.2.0",
    "react-native-firebase": "^5.5.6",
    "react-native-fs": "^2.13.3",
    "react-native-gesture-handler": "^1.1.3",
    "react-native-languages": "^3.0.2",
    "react-native-modal-selector": "1.0.3",
    "react-native-sentry": "^0.40.3",
    "react-native-settings-list": "^1.8.0",
    "react-native-share": "^1.2.1",
    "react-native-snackbar": "^0.5.5",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^9.4.0",
    "react-native-svg-web": "^1.0.1",
    "react-native-vector-icons": "oblador/react-native-vector-icons#master",
    "react-native-version-number": "^0.3.5",
    "react-native-web": "^0.11.2",
    "react-native-web-responsive": "^1.0.5",
    "react-navigation": "^3.8.1",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.2",
    "reselect": "^4.0.0",
    "ts-jest": "^24.0.2",
    "url-loader": "^1.1.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.14",
    "@types/react-native": "^0.57.60",
    "@types/react-native-fs": "^2.8.3",
    "@types/react-redux": "^7.0.7",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.4",
    "@types/realm": "^1.13.0",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-saga": "^0.10.5",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-native-web": "^0.11.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "cache-loader": "^4.0.1",
    "concurrently": "^4.1.1",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.53.1",
    "react-test-renderer": "16.8.3",
    "ts-loader": "^5.1.3",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.3.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  }
